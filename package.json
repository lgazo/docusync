{
  "name": "docusync",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Ladislav Gazo",
  "license": "MIT",
  "bin": {
    "docusync": "./build/index.js"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "tsc --project ./",
    "start": "node build/",
    "dev:addResource": "tsc --project ./ &  node build/index.js addResource https://www.xwiki.org/xwiki/bin/view/Documentation/UserGuide",
    "dev:help": "tsc --project ./ &  node build/index.js --help",
    "dev:clean": "tsc --project ./ &  node build/index.js",
    "dev:download": "tsc --project ./ &  node build/index.js download"
  },
  "devDependencies": {
    "@types/node": "14.6.4",
    "@types/ramda": "0.27.16",
    "@types/yargs": "15.0.5",
    "nodemon": "2.0.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "clui": "0.3.6",
    "configstore": "5.0.1",
    "figlet": "1.5.0",
    "inquirer": "7.3.3",
    "node-fetch": "2.6.1",
    "ramda": "^0.27.1",
    "yargs": "16.0.3"
  }
}
