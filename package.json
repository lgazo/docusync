{
  "name": "docusync",
  "version": "1.0.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "author": "Ladislav Gazo",
  "license": "MIT",
  "bin": {
    "docusync": "./build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgazo/docusync.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "start": "node build/",
    "dev:addResource": "tsc --project ./ &  node build/index.js addResource https://www.xwiki.org/xwiki/bin/view/Documentation/UserGuide",
    "dev:help": "tsc --project ./ &  node build/index.js --help",
    "dev:clean": "tsc --project ./ &  node build/index.js",
    "dev:download": "tsc --project ./ &  node build/index.js download",
    "test": "echo \"No test specified\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "devDependencies": {
    "@types/node": "14.6.4",
    "@types/ramda": "0.27.16",
    "@types/yargs": "15.0.5",
    "nodemon": "2.0.4",
    "prettier": "2.1.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "clui": "0.3.6",
    "configstore": "5.0.1",
    "figlet": "1.5.0",
    "inquirer": "7.3.3",
    "node-fetch": "2.6.1",
    "ramda": "0.27.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "yargs": "16.0.3"
  },
  "files": [
    "build/**/*"
  ]
}